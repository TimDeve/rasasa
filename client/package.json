{
  "name": "rasasa-client",
  "version": "0.1.0",
  "description": "A RSS Reader",
  "author": "Timothee Deve",
  "scripts": {
    "precommit": "npm run lint && npm run build",
    "start:dev:server": "webpack-dev-server",
    "start:dev:ts": "tsc --watch",
    "build": "webpack",
    "lint": "npm run lint:prettier && npm run lint:ts && npm run lint:tslint",
    "lint:prettier": "prettier src/**/*.{ts,tsx,js,jsx,json,scss,css} --write",
    "lint:ts": "tsc"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              "last 3 chrome versions",
              "last 3 firefox versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "prettier": {
    "printWidth": 110,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@hot-loader/react-dom": "^16.9.0",
    "@types/classnames": "2.2.7",
    "@types/lodash-es": "4.17.3",
    "@types/query-string": "6.3.0",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/react-router-dom": "5.1.0",
    "@types/sanitize-html": "1.20.2",
    "@types/uuid": "3.4.5",
    "@types/workbox-window": "4.3.2",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^2.1.1",
    "dexie": "^2.0.4",
    "hmr-filter-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash-es": "^4.17.15",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "query-string": "^6.8.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.14",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scrolllock": "^4.0.1",
    "sanitize-html": "^1.20.1",
    "sass-loader": "^7.3.1",
    "service-worker-loader": "^3.3.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-background-sync": "^4.3.1",
    "workbox-build": "^4.3.1",
    "workbox-core": "^4.3.1",
    "workbox-routing": "^4.3.1",
    "workbox-strategies": "^4.3.1",
    "workbox-window": "^4.3.1"
  }
}
