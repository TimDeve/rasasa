{
  "name": "rasasa-client",
  "version": "0.1.0",
  "description": "A RSS Reader",
  "author": "Timothee Deve",
  "scripts": {
    "precommit": "npm run lint && npm run build",
    "start:dev:server": "webpack-dev-server",
    "start:dev:ts": "tsc --watch",
    "build": "webpack",
    "lint": "npm run lint:prettier && npm run lint:ts",
    "lint:prettier": "prettier src/**/*.{ts,tsx,js,jsx,json,scss,css} --write",
    "lint:ts": "tsc"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              "last 3 chrome versions",
              "last 3 firefox versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "prettier": {
    "printWidth": 110,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "2.2.10",
    "@types/query-string": "6.3.0",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.6",
    "@types/sanitize-html": "1.27.0",
    "@types/uuid": "8.3.0",
    "@types/workbox-window": "4.3.3",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^4.3.0",
    "dexie": "^3.0.2",
    "hmr-filter-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "query-string": "^6.13.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scrolllock": "^5.0.1",
    "sanitize-html": "^2.1.0",
    "sass-loader": "^10.0.3",
    "service-worker-loader": "^4.0.2",
    "source-map-loader": "^1.1.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.0.3",
    "uuid": "^8.3.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-background-sync": "^4.3.1",
    "workbox-build": "^4.3.1",
    "workbox-core": "^4.3.1",
    "workbox-routing": "^4.3.1",
    "workbox-strategies": "^4.3.1",
    "workbox-window": "^4.3.1"
  }
}
